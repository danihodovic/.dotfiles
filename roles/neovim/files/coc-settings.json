{
  "coc.preferences.formatOnSave":true,
  "coc.preferences.formatOnType":false,
  "coc.source.emoji.filetypes":[
    "markdown",
    "htmldjango",
    "html"
  ],
  "coc.source.word.filetypes":[
    "markdown",
    "txt"
  ],
  "diagnostic.autoRefresh":true,
  "diagnostic.enableHighlightLineNumber":false,
  "diagnostic.locationlistUpdate":true,
  "diagnostic.virtualText":true,
  "diagnostic.virtualTextCurrentLineOnly":false,
  "diagnostic.virtualTextPrefix":"âœ— ",
  "diagnostic.virtualTextWinCol":80,
  "html.autoClosingTags":true,
  "html.format.indentHandlebars":true,
  "html.format.indentInnerHtml":true,
  "htmldjango.djlint.addFormatterArgs":[
    "--max-line-length",
    "100"
  ],
  "htmldjango.formatting.provider":"djlint",
  "inlayHint.enable":false,
  "javascript.format.enable":false,
  "json.format.enable":false,
  "languageserver":{
    "terraform":{
      "command":"terraform-ls",
      "args":[
        "serve"
      ],
      "filetypes":[
        "terraform",
        "tf"
      ],
      "initializationOptions":{

      },
      "settings":{

      }
    },
    "jsonnet":{
      "command":"jsonnet-language-server",
      "args":[
        "-t"
      ],
      "rootPatterns":[
        ".git/",
        "jsonnetfile.json"
      ],
      "filetypes":[
        "jsonnet",
        "libsonnet"
      ]
    }
  },
  "pairs.disableLanguages":[
    "html",
    "htmldjango"
  ],
  "pyright.disableDiagnostics":true,
  "pyright.organizeimports.provider":"isort",
  "python.analysis.diagnosticSeverityOverrides":{
    "reportGeneralTypeIssues":"none"
  },
  "python.analysis.typeCheckingMode":"off",
  "python.formatting.blackdHTTPHeaders":{
    "X-Fast-Or-Safe":"fast"
  },
  "python.formatting.provider":"blackd",
  "python.linting.enabled":true,
  "python.linting.mypyEnabled":false,
  "python.linting.pylintEnabled":true,
  "suggest.snippetIndicator":"ðŸ”¨",
  "tailwindCSS.emmetCompletions":true,
  "typescript.format.enable":false,
  "typescript.validate.enable":false,
  "workspace.workspaceFolderFallbackCwd":false,
  "yaml.format.enable":false,
  "yaml.format.singleQuote":false,
  "yaml.validate":false
}
